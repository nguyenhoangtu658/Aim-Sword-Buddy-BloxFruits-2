local function fallbackHitbox()

    if game.CoreGui:FindFirstChild("FallbackHitboxGui") then
        game.CoreGui.FallbackHitboxGui:Destroy()
    end
    local gui = Instance.new("ScreenGui", game.CoreGui)
    gui.Name = "FallbackHitboxGui"
    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0, 230, 0, 160)
    frame.Position = UDim2.new(0.5, 0, 0.5, 0)
    frame.AnchorPoint = Vector2.new(0.5,0.5)
    frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0,12)

    local title = Instance.new("TextLabel", frame)
    title.Size = UDim2.new(1,0,0.2,0)
    title.Position = UDim2.new(0,0,0,0)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBold
    title.Text = "Hitbox GUI"
    title.TextColor3 = Color3.fromRGB(255,255,255)
    title.TextScaled = true
    title.TextXAlignment = Enum.TextXAlignment.Center

    local toggle = Instance.new("TextButton", frame)
    toggle.Size = UDim2.new(0.7,0,0.22,0)
    toggle.Position = UDim2.new(0.15,0,0.3,0)
    toggle.Text = "OFF"
    toggle.Font = Enum.Font.GothamBold
    toggle.TextSize = 18
    toggle.BackgroundColor3 = Color3.fromRGB(255,106,106)
    Instance.new("UICorner", toggle).CornerRadius = UDim.new(0,8)

    local valLbl = Instance.new("TextLabel", frame)
    valLbl.Size = UDim2.new(0.3,0,0.2,0)
    valLbl.Position = UDim2.new(0.35,0,0.65,0)
    valLbl.Text = "5"
    valLbl.Font = Enum.Font.GothamBold
    valLbl.TextScaled = true
    valLbl.BackgroundColor3 = Color3.fromRGB(50,50,50)
    Instance.new("UICorner", valLbl).CornerRadius = UDim.new(0,6)

    local minus = Instance.new("TextButton", frame)
    minus.Size = UDim2.new(0.2,0,0.2,0)
    minus.Position = UDim2.new(0.1,0,0.65,0)
    minus.Text = "-"
    Instance.new("UICorner", minus).CornerRadius = UDim.new(0,6)

    local plus = Instance.new("TextButton", frame)
    plus.Size = UDim2.new(0.2,0,0.2,0)
    plus.Position = UDim2.new(0.7,0,0.65,0)
    plus.Text = "+"
    Instance.new("UICorner", plus).CornerRadius = UDim.new(0,6)

    local on = false
    local cur = 5
    local function applyHitbox()
        for _,p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                local hrp = p.Character.HumanoidRootPart
                if on then
                    hrp.Size = Vector3.new(cur,cur,cur)
                    hrp.Transparency = 0.5
                    hrp.Material = Enum.Material.Neon
                    hrp.Color = Color3.fromRGB(255,0,0)
                    hrp.CanCollide = false
                else
                    hrp.Size = Vector3.new(2,2,1)
                    hrp.Transparency = 1
                    hrp.Material = Enum.Material.Plastic
                    hrp.Color = Color3.fromRGB(163,162,165)
                    hrp.CanCollide = true
                end
            end
        end
    end

    toggle.MouseButton1Click:Connect(function()
        on = not on
        toggle.Text = on and "ON" or "OFF"
        toggle.BackgroundColor3 = on and Color3.fromRGB(154,255,154) or Color3.fromRGB(255,106,106)
        applyHitbox()
    end)
    minus.MouseButton1Click:Connect(function()
        cur = math.max(2, cur - 1)
        valLbl.Text = tostring(cur)
        if on then applyHitbox() end
    end)
    plus.MouseButton1Click:Connect(function()
        cur = math.min(20, cur + 1)
        valLbl.Text = tostring(cur)
        if on then applyHitbox() end
    end)
end

hitboxBtn.MouseButton1Click:Connect(function()
    local tried = false
    local urls = {
        "https://raw.githubusercontent.com/nguyenhoangtu658/Hitbox-v1.0/refs/heads/main/hitbox-gui.txt.bak",
        "https://raw.githubusercontent.com/nguyenhoangtu658/Hitbox-v1.0/main/hitbox-gui.txt.bak"
    }
    for _, url in ipairs(urls) do
        local ok, res = pcall(function() return game:HttpGet(url, true) end)
        if ok and res and type(res) == "string" and #res > 10 then
            local sOk, sErr = pcall(function() loadstring(res)() end)
            if sOk then
                tried = true
                break
            end
        end
    end
    if not tried then
    
        fallbackHitbox()
    end
end)

